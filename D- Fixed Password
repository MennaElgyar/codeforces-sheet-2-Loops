#include <iostream>
#include<math.h>
#include<cmath>
using namespace std;
 
int main() {
    int password ;
    while(true){
        cin>>password;
        if(password==1999){
           cout<<"Correct";
           break;
        }
        else{
            cout<<"Wrong"<<endl;
        }
    }
    return 0;
}
